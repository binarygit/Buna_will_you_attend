<h1>Login</h1>

<%= form_with url: '/sessions', method: 'post' do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit 'Log In', data: {'turbo-action': :replace} %>
  </div>
<% end %>
